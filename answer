Week 5 : Programming Assignment 4

class A implements Number
 {
	 int k;
         int square;
	 public int findSqr(int k)
  {
		 square=k*k;
         return square;
  }
}

Week 5 : Programming Assignment 5

class B implements GCD {
    int no1,no2;
    public int findGCD(int no1, int no2){
		if(no1==0&& no2==0)
        {
			return(-1);
		}
		else if(no2 == 0)
        {
			return(no1);
		}
		else {
			return findGCD(no2, no1%no2);
		}
        }
 }
 
